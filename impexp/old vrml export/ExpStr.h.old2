#ifndef _EXPSTR_#define _EXPSTR_#define EXPSTR#include "VIO.h"#include "stdio.h"#include "string.h"	// End of line character...//#define EOL "\x0d"#define EOL "\n"#define TAB	"\x09"#define QUOTE "\""#define OBRACE "{"#define CBRACE "}"#define BIT32		65535		// PRO25#define BIT32F		65535.0		// PRO25OSErr IOString(V_Buffer *buf,char *str);OSErr IOIndentString(V_Buffer *buf, char *str, int indent);#if !THINK_Cinline OSErr IOString(V_Buffer *buf,char *str) 	{return buf->IOData(str,strlen(str));}inline OSErr IOIndentString(V_Buffer *buf, char *str, int indent)	{	int i;	OSErr err=0;	if(indent<0)indent=0;	for(i=0;i<indent && !err;i++)err=IOString(buf,TAB);	if(!err)		err=IOString(buf,str);	return err;	}#endif#endif // _EXPSTR_